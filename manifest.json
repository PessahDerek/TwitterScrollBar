{
  "manifest_version": 2,
  "name": "TwitterScrollBar",
  "version": "1.0",
  "description": "A Chrome extension to style the Twitter navigation bar",
  "permissions": ["https://twitter.com/*"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["styles.css"]
    }
  ]
}